generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model usuarios {
  id       Int           @id @default(autoincrement())
  name     String
  idade    String
  estado   String
  cidade   String
  email    String        @unique
  senha    String
  carteira String?       @default("0")
  nivel    String?       @default("1")
  ativo    Boolean?      @default(true)
  criado   DateTime?     @default(now())
  tipo     usuarios_tipo @default(USER)
  des1     String?
  des2     String?
  des3     String?
  des4     String?
  des5     String?
  des6     String?
  des7     String?
  des8     String?
  des9     String?
}

enum usuarios_tipo {
  USER
  ADMIN
}
